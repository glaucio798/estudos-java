package application;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Maria1");
		list.add("Maria2");
		list.add("Maria3");
		list.add(2, "Joao");
		
		System.out.println(list.size());
		
		for(String x : list) {
			System.out.println(x);
		}
	}

}
